{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "datr.py",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"program": "${workspaceRoot}/datr.py",
			"args": ["-n", "50", "-s", "car, funny", "-f", "-i", "l"],
