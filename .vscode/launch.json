{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "datr.py",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"program": "${workspaceRoot}/datr.py",
			"args": ["-n", "10000", "-s", "dog", "-t", "20"],
			"pythonPath": "/usr/bin/python2",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		}
	]
}